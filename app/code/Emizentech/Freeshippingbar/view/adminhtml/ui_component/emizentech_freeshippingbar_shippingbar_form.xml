<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">emizentech_freeshippingbar_shippingbar_form.shippingbar_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Emizentech\Freeshippingbar\Block\Adminhtml\Shippingbar\Edit\BackButton"/>
			<button name="delete" class="Emizentech\Freeshippingbar\Block\Adminhtml\Shippingbar\Edit\DeleteButton"/>
			<button name="save" class="Emizentech\Freeshippingbar\Block\Adminhtml\Shippingbar\Edit\SaveButton"/>
			<button name="save_and_continue" class="Emizentech\Freeshippingbar\Block\Adminhtml\Shippingbar\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>emizentech_freeshippingbar_shippingbar_form</namespace>
		<dataScope>data</dataScope>
		 <layout>
            <!-- I assume there are other options; set the layout handle accordingly -->
            <navContainerName>left</navContainerName>
            <type>tabs</type>
        </layout>
		<deps>
			<dep>emizentech_freeshippingbar_shippingbar_form.shippingbar_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="shippingbar_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="shippingbar_form_data_source" class="Emizentech\Freeshippingbar\Model\Shippingbar\DataProvider">
			<settings>
				<requestFieldName>shippingbar_id</requestFieldName>
				<primaryFieldName>shippingbar_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="name" formElement="textarea" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Shippingbar</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">name</label>
				<dataScope>name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="status" formElement="input" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
